
int mult(int[2] arr, int size) {
    int i = 0;
    while i < size {
        int j = 0;
        while j < size {
            arr[j,i] = arr[j,i]*2;
            j = j + 1;
        }
        i = i + 1;
    }
    return 0;
}

int main() {
    int size = 5000;
    int[2] arr = new int[size, size];
    int copycount = 100;
    while copycount > 0 {
        mult(arr, size);
        copycount = copycount - 1;
    }
    return 0;
}