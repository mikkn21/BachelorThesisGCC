int partition(int[1] arr, int low, int high) {
    int pivot = arr[high];
    int i = (low - 1); 
    int j = low;
    while (j <= high-1) {
        if (arr[j] <= pivot) {
            i = i+1; 
            int temp1 = arr[i];
            arr[i] = arr[j];
            arr[j] = temp1;
        }
        j = j+1;
    }
    int temp2 = arr[i + 1];
    arr[i + 1] = arr[high];
    arr[high] = temp2;
    return (i + 1);

}

