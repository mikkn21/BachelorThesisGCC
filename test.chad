int partition(int[1] arr, int low, int high) {
    int pivot = arr[high];
    int i = (low - 1); 
    int j = low;
    while (j <= high-1) {
        if (arr[j] <= pivot) {
            i = i+1; 
            int temp1 = arr[i];
            arr[i] = arr[j];
            arr[j] = temp1;
        }
        j = j+1;
    }
    int temp2 = arr[i + 1];
    arr[i + 1] = arr[high];
    arr[high] = temp2;
    return (i + 1);
}

int quicksort(int[1] arr, int low, int high) {
    if (low < high) {
        int pi = partition(arr, low, high);

        quicksort(arr, low, pi - 1);
        quicksort(arr, pi + 1, high);
    }
    return 0;
}

int main() {
    int arr_size = 100000;
    int[1] arr = new int[arr_size];
	
	int j = 0;
	while j < arr_size {
		arr[j] = j;
		j = j + 1;
	}



    quicksort(arr, 0, arr_size - 1);
    
    int i = 0;
    while i < arr_size {
        print(arr[i]);
        i = i + 1;
    }

    return 0;
}
