int test1() {
    int x = 2;
    while x < 5 {
        print(x);
        x = x + 1;
    }
    return 0;
}


int test2() {
    int x = 2;
    while false {
        print(x);
        x = x + 1;
    }
    return 0;
}

int test3() {
    bool x = true;
    while x {
        print(x);
        x = false;
    }
    return 0;
}


int test4() {
    int x = 1;
    while x > 1 {
        print(x);
        x = x-1;
    }
    return 0;
}


int main() {
    test1();
    test2();
    test3();
    test4();
    return 0;
}

